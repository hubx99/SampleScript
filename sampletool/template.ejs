<html>
<head>
<meta charset="UTF-8">
<style>
table {
	width: 100%;
}
table tr td,
table tr th {
	padding: 10px;
	border: 1px solid #888;
	border-radius: 5px;
}
.head {
	margin: 10px;
}
.head-right {
	float: right;
}
.number {
	text-align: right;
}
.hot {
	background: #fee;
}
.cold {
	background: #eef;
}
.warn {
	background: #ffe;
}
</style>
</head>
<body>
	<h4 class="head">
		title
		<div class="head-right">
			<%= time %>
		</div>
	</h4>
	<table>
		<thead>
			<th>#</th>
			<th>fname</th>
			<th>value</th>
			<th>value</th>
			<th>fsize-a</th>
			<th>fsize-b</th>
			<th>fsize-delta</th>
			<!-- th>fsize %</th -->
		</thead>
		<tbody>
			<% list.forEach(function(obj, i) { %>
			<tr class="<%= obj.status %>">
				<td class="number"><%= (i+1) %></td>
				<td><%= obj.fname %></td>
				<td><%= obj.cell["B2"] %></td>
				<td><%= obj.cell["B2"] %></td>
				<td class="number"><%= obj.hist %></td>
				<td class="number"><%= obj.fsize %></td>
				<td class="number"><%= obj.diff %></td>
				<!-- td class="number"><%= obj.sizePercent %></td -->
			</tr>
			<% }); %>
		</tbody>
	</table>
</body>
</html>
